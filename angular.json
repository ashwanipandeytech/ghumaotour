{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "cli": {
        "analytics": false
    },
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "ghumao": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                },
                "@schematics/angular:application": {
                    "strict": true
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/ghumao",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"

                        ],
                        "styles": [
                            "src/styles.scss",
                            "src/assets/css/bootstrap.min.css",
                            "src/assets/css/all.min.css",
                            "src/assets/css/jquery-ui.min.css",
                            "src/assets/css/modal-video.min.css",
                            "src/assets/css/lightbox.min.css",
                            "src/assets/css/slick.css",
                            "src/assets/css/slick-theme.css",
                            "src/assets/css/style.css",
                            "node_modules/slick-carousel/slick/slick.scss",
                            "node_modules/slick-carousel/slick/slick-theme.scss",
                            "node_modules/ngx-toastr/toastr.css"
                        ],
                        "scripts": [
                            "src/assets/js/email-decode.min.js",
                            "src/assets/js/jquery.js",
                            "src/assets/js/waypoints.min.js",
                            "src/assets/js/bootstrap.min.js",

                            "node_modules/jquery/dist/jquery.min.js",
                            "node_modules/slick-carousel/slick/slick.min.js",
                            "src/assets/js/loopcounter.js",
                            "src/assets/js/jquery.counterup.js",
                            "src/assets/js/jquery-modal-video.min.js",
                            "src/assets/js/masonry.pkgd.min.js",
                            "src/assets/js/lightbox.min.js",
                            "src/assets/js/slick.min.js",
                            "src/assets/js/jquery.slicknav.js",
                            "src/assets/js/custom.min.js",
                            "src/assets/js/custom.js"

                        ]
                    },
                    "configurations": {
                        "production": {
                            "budgets": [{
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "100mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "200kb",
                                    "maximumError": "400kb"
                                }
                            ],
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.prod.ts"
                            }],
                            "outputHashing": "all"
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "browserTarget": "ghumao:build:production"
                        },
                        "development": {
                            "browserTarget": "ghumao:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "ghumao:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.spec.json",
                        "karmaConfig": "karma.conf.js",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "src/styles.scss"
                        ],
                        "scripts": []
                    }
                }
            }
        }
    },
    "defaultProject": "ghumao"
}